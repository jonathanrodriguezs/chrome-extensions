{
  "manifest_version": 2,
  "name": "Adfly redirector",
  "description": "Redirects to shinkred url",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://*/pushredirect/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}